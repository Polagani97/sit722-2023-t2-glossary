<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>

  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= sub %></p>

    <table>
      <tr>
        <th>ID#</th>
        <th>Term</th>
        <th>Explanation</th>
        <th>Acknowledgement</th>
      </tr>
      <% names.forEach((name) => {  console.log(name) %>
      <tr>
        <td> <%= name.id %> </td>
        <td> <%= name.Term %> </td>
        <td> <%= name.Explanation %> </td>
        <td> <%= name.Acknowledgement %> </td>
     </tr>
    <% }); %>
    </table>
  </body>
</html>
